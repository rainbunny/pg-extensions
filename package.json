{
  "author": "Thinh Tran",
  "bugs": {
    "url": "https://github.com/rainbunny/pg-extensions/issues"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "pg": "^8.5.1"
  },
  "description": "Add more functions into the original pg package",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.8",
    "@types/pg": "^7.14.11",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-null": "^1.0.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.5"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/rainbunny/pg-extensions#readme",
  "keywords": [
    "pg",
    "postgres",
    "typescript"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "@rainbunny/pg-extensions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainbunny/pg-extensions.git"
  },
  "scripts": {
    "build": "rm -r -f lib && babel src --out-dir lib --extensions \".ts\" --source-maps inline --copy-files && tsc --project tsconfig.definition.json",
    "lint": "prettier --check . && eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit",
    "lint-fix": "prettier --write . && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "postversion": "git push && git push --tags",
    "prepare": "yarn build",
    "prepublishOnly": "yarn validate",
    "preversion": "yarn validate",
    "test": "jest",
    "update-version": "ts-node scripts/update-version",
    "validate": "yarn lint && yarn test",
    "version": "yarn validate && git add -A src"
  },
  "types": "lib/index.d.ts",
  "version": "0.1.0"
}
